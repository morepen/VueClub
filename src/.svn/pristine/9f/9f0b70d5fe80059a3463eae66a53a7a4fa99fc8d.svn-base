    right: 0;
    bottom: auto;
    min-height: 80px;
    text-align: center;
    z-index: 11;
    -webkit-transition: top .4s ease;
    -moz-transition: top .4s ease;
    -ms-transition: top .4s ease;
    -o-transition: top .4s ease;
    transition: top .4s ease;
}
.noticeWrap.error{
    background-color: #b86158;
}
.noticeWrap.success{
    background-color: #7bcaa5;
}
.notice{
    display: table;
    height: 80px;
    margin: 0 auto;
}
.noticeInner{
    display: table-cell;
    vertical-align: middle;
    color: #fff;
}

/*===========*/
/* Intro */
/*===========*/
.intro-wrap,
.intro-wrap *,
.intro-wrap *:before,
.intro-wrap *:after {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.intro-wrap {

    overflow: auto;
    -webkit-overflow-scrolling: touch;
    z-index: 10000;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    /*display: none;*/
}

.intro-wrap-overlay {
    position: fixed;
    background: #e5e4db;
    /*background:-moz-radial-gradient(50% 50%, rgba(255,255,255,0.9), rgba(255,255,255,1));
    background:-webkit-radial-gradient(50% 50%, rgba(255,255,255,0.9), rgba(255,255,255,1));
    background:-webkit-gradient(radial, 0 50%, 0 50%, from(rgba(255,255,255,0.9)), to(rgba(255,255,255,1)));*/
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    height: 100%;
    z-index: -1;
    opacity: 0;

    -webkit-transition: opacity .4s ease;
    -moz-transition: opacity .4s ease;
    -ms-transition: opacity .4s ease;
    -o-transition: opacity .4s ease;
    transition: opacity .4s ease;
}

.intro-wrap-content {
    text-align: center;
    height: 100%;
    background: none;
}

.intro-wrap-content:before {
    content: '';
    display: inline-block;
    height: 100%;
    vertical-align: middle;
}

#IntroInner{
    display: inline-block;
    padding: 0 40px;
    vertical-align: middle;
    outline: 0;
    overflow: hidden;
    min-width: 300px;
    max-width: 650px;
    /*"Roboto",Helvetica,"Lucida Grande","Luxi Sans",Arial,"Hiragino Sans GB",STHeiti,"Microsoft YaHei","Wenquanyi Micro Hei","WenQuanYi Micro Hei Mono","WenQuanYi Zen Hei","WenQuanYi Zen Hei Mono",LiGothicMed*/
    font-size: 16px;
    color: #514f47;
    line-height: 1.5;
    background: none;
    text-align: left;
    /*border-radius: 5px;*/
    opacity: 0;
    text-rendering: optimizeLegibility;
    /*-webkit-font-smoothing: antialiased;*/
    -webkit-font-smoothing: subpixel-antialiased;

}
.intro-wrap-content > .intro-wrap-close {
    display: none;
    width: 30px;
    height: 30px;
    line-height: 26px;
    text-align: center;
    cursor: pointer;
    position: absolute;
    z-index: 3;
    top: 4px;
    right: 21px;
    color: #fff;
    font-size: 24px;
    background-color: #cecdc5;
    border-radius: 3px;
}
.intro-wrap-content > .intro-wrap-close:hover {
    background-color: #c4c3bb;
    color: #fff;
}
.intro-wrap-content > .intro-wrap-close:before {
    font-family: 'Helvetica', Arial, sans-serif;
    content: '\00D7';
    cursor: pointer;
}

.intro-wrap.intro-wrap-closing .intro-wrap-overlay,
.intro-wrap.intro-wrap-closing .intro-wrap-content{
    opacity: 0;
}

body.intro-wrap-open {
    height: 100%;
    overflow: hidden;
}
body.intro-wrap-open .intro-wrap-close{
    display: block;
}
body.intro-wrap-open .intro-wrap-overlay{
    opacity: 1;
}
body.intro-wrap-open  .intro-wrap-content #IntroInner{
    opacity: 1;
    -webkit-transform-origin: bottom center;
    -moz-transform-origin: bottom center;
    -o-transform-origin: bottom center;
    transform-origin: bottom center;
    -webkit-animation: scale-fade 300ms cubic-bezier(0.8,0.02,0.45,0.91);
    -moz-animation: scale-fade 300ms cubic-bezier(0.8,0.02,0.45,0.91);
    -ms-animation: scale-fade 300ms cubic-bezier(0.8,0.02,0.45,0.91);
    -o-animation: scale-fade 300ms cubic-bezier(0.8,0.02,0.45,0.91);
    animation: scale-fade 300ms cubic-bezier(0.8,0.02,0.45,0.91);
}

@-webkit-keyframes scale-fade {
    0% {
        filter: alpha(opacity=0);
        -khtml-opacity: 0;
        -moz-opacity: 0;
        opacity: 0;
        -webkit-transform: scale(0.8) rotateX(-40deg);
        -moz-transform: scale(0.8) rotateX(-40deg);
        -o-transform: scale(0.8) rotateX(-40deg);
        transform: scale(0.8) rotateX(-40deg)
    }

    50% {
        filter: alpha(opacity=100);
        -khtml-opacity: 1;
        -moz-opacity: 1;
        opacity: 1
    }

    70% {
        -webkit-transform: scale(1.05) rotateX(0);
        -moz-transform: scale(1.05) rotateX(0);
        -o-transform: scale(1.05) rotateX(0);
        transform: scale(1.05) rotateX(0)
    }

    100% {
        -webkit-transform: scale(1) rotateX(0);
        -moz-transform: scale(1) rotateX(0);
        -o-transform: scale(1) rotateX(0);
        transform: scale(1) rotateX(0)
    }
}

@-moz-keyframes scale-fade {
    0% {
        filter: alpha(opacity=0);
        -khtml-opacity: 0;
        -moz-opacity: 0;
        opacity: 0;
        -webkit-transform: scale(0.8) rotateX(-40deg);
        -moz-transform: scale(0.8) rotateX(-40deg);
        -o-transform: scale(0.8) rotateX(-40deg);
        transform: scale(0.8) rotateX(-40deg)
    }

    50% {
        filter: alpha(opacity=100);
        -khtml-opacity: 1;
        -moz-opacity: 1;
        opacity: 1
    }

    70% {
        -webkit-transform: scale(1.05) rotateX(0);
        -moz-transform: scale(1.05) rotateX(0);
        -o-transform: scale(1.05) rotateX(0);
        transform: scale(1.05) rotateX(0)
    }

    100% {
        -webkit-transform: scale(1) rotateX(0);
        -moz-transform: scale(1) rotateX(0);
        -o-transform: scale(1) rotateX(0);
        transform: scale(1) rotateX(0)
    }
}

@-o-keyframes scale-fade {
    0% {
        filter: alpha(opacity=0);
        -khtml-opacity: 0;
        -moz-opacity: 0;
        opacity: 0;
        -webkit-transform: scale(0.8) rotateX(-40deg);
        -moz-transform: scale(0.8) rotateX(-40deg);
        -o-transform: scale(0.8) rotateX(-40deg);
        transform: scale(0.8) rotateX(-40deg)
    }

    50% {
        filter: alpha(opacity=100);
        -khtml-opacity: 1;
        -moz-opacity: 1;
        opacity: 1
    }

    70% {
        -webkit-transform: scale(1.05) rotateX(0);
        -moz-transform: scale(1.05) rotateX(0);
        -o-transform: scale(1.05) rotateX(0);
        transform: scale(1.05) rotateX(0)
    }

    100% {
        -webkit-transform: scale(1) rotateX(0);
        -moz-transform: scale(1) rotateX(0);
        -o-transform: scale(1) rotateX(0);
        transform: scale(1) rotateX(0)
    }
}

@keyframes scale-fade {
    0% {
        filter: alpha(opacity=0);
        -khtml-opacity: 0;
        -moz-opacity: 0;
        opacity: 0;
        -webkit-transform: scale(0.8) rotateX(-40deg);
        -moz-transform: scale(0.8) rotateX(-40deg);
        -o-transform: scale(0.8) rotateX(-40deg);
        transform: scale(0.8) rotateX(-40deg)
    }

    50% {
        filter: alpha(opacity=100);
        -khtml-opacity: 1;
        -moz-opacity: 1;
        opacity: 1
    }

    70% {
        -webkit-transform: scale(1.05) rotateX(0);
        -moz-transform: scale(1.05) rotateX(0);
        -o-transform: scale(1.05) rotateX(0);
        transform: scale(1.05) rotateX(0)
    }

    100% {
        -webkit-transform: scale(1) rotateX(0);
        -moz-transform: scale(1) rotateX(0);
        -o-transform: scale(1) rotateX(0);
        transform: scale(1) rotateX(0)
    }
}

