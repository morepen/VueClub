    background: -ms-linear-gradient(90deg, rgba(0, 0, 0, 0.5) 0%, rgba(0, 0, 0, 0) 50%);
    background: linear-gradient(180deg, rgba(0, 0, 0, 0.5) 0%, rgba(0, 0, 0, 0) 50%);
    z-index: 1;
}

.musicBox:after{
    content: "";
    position: absolute;
    top: 0;
    right: 0;
