/*waiting for delete*/
.chat-box.waiting{
    -webkit-animation-name:scaleBox;
    -webkit-animation-duration:0.4s;
    -webkit-animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count:1;

    -moz-animation-name:scaleBox;
    -moz-animation-duration:0.4s;
    -moz-animation-timing-function: ease-in-out;
    -moz-animation-iteration-count:1;
}

.chat-box.waiting p{
    background-color: rgba(255,255,255,0.7);
}

.chat-box .confirm{
    position: absolute;
    right: 0;
    top: 0;
    z-index: 4;
}

.chat-box.waiting .confirm{
    -webkit-animation-name:scaleConfirmBox;
    -webkit-animation-duration:0.2s;
    -webkit-animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count:1;

    -moz-animation-name:scaleConfirmBox;
    -moz-animation-duration:0.2s;
    -moz-animation-timing-function: ease-in-out;
    -moz-animation-iteration-count:1;
}

/* keyframes scaleBox for .chat-box animate */
@-webkit-keyframes scaleBox{
    0%{
        -webkit-transform:scale(1);
    }
    50%{
        -webkit-transform:scale(1.1);
    }
    100%{
        -webkit-transform:scale(1);
    }
}
@-moz-keyframes scaleBox{
    0%{
        -moz-transform:scale(1);
    }
    50%{
        -moz-transform:scale(1.1);
    }
    100%{
        -moz-transform:scale(1);
    }
}

/* keyframes scaleConfirmBox for .confirm animate */
@-webkit-keyframes scaleConfirmBox{
    0%{
        -webkit-transform:scale(0.1);
        opacity: 0;
    }
    100%{
        -webkit-transform:scale(1);
        opacity: 1;
    }
}
@-moz-keyframes scaleConfirmBox{
    0%{
        -moz-transform:scale(0.1);
        opacity: 0;
    }
    100%{
        -moz-transform:scale(1);
        opacity: 1;
    }
}

.chat-box .confirm i{
    margin-right: 4px;
    opacity: 0;
    cursor: pointer;
    border-radius: 50%;
    padding: 2px 3px 1px 3px;
    background-color: #fff;
    box-shadow: 0 0 3px rgba(0,0,0,0.4);
    transition:all 0.3s ease;
}
.chat-box.waiting .confirm i{
    opacity: 0.7;
    -moz-transform:scale(0.8);
    -webkit-transform:scale(0.8);
    transform:scale(0.8);
}
.chat-box.waiting .confirm i:hover{
    -moz-transform:scale(1);
    -webkit-transform:scale(1);
    transform:scale(1);
}

.delete{
    color: #b86158;
}
.cancel{
    color: #555;
}

.chat-box.animate p{
    -webkit-animation-name:rotateLeft;
    -webkit-animation-duration:0.25s;
    -webkit-animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count:infinite;

    -moz-animation-name:rotateLeft;
    -moz-animation-duration:0.25s;
    -moz-animation-timing-function: ease-in-out;
    -moz-animation-iteration-count:infinite;
}

@-webkit-keyframes rotateLeft{
    0%{
        -webkit-transform:rotate(0);
        margin-left: -1px;
        margin-top:-1px;
    }
    25%{
        -webkit-transform:rotate(-0.6deg);
        margin-left: 0px;
        margin-top:0px;
    }
    50%{
        -webkit-transform:rotate(0deg);
        margin-left: 1px;
        margin-top:1px;
    }
    75%{
        -webkit-transform:rotate(0.6deg);
        margin-left: 0px;
        margin-top:0px;
    }
    100%{
        -webkit-transform:rotate(0deg);
        margin-left: -1px;
        margin-top:-1px;
    }
}
@-moz-keyframes rotateLeft{
    0%{
        -moz-transform:rotate(0);
        margin-left: -1px;
        margin-top:-1px;
    }
    25%{
        -moz-transform:rotate(-0.6deg);
        margin-left: 0px;
        margin-top:0px;
    }
    50%{
        -moz-transform:rotate(0deg);
        margin-left: 1px;
        margin-top:1px;
    }
    75%{
        -moz-transform:rotate(0.6deg);
        margin-left: 0px;
        margin-top:0px;
    }
    100%{
        -moz-transform:rotate(0deg);
        margin-left: -1px;
        margin-top:-1px;
    }
}


