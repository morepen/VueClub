             Public
*******************************/
.hide{display: none;}
.show{display: block;}
.f_w_100{font-weight: 100;}

.bg-overlay{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left:0;
    background: rgba(0,0,0,0.2);
    background:-moz-radial-gradient(50% 20%, rgba(0,0,0,0.1), rgba(0,0,0,0.8));
    background:-webkit-radial-gradient(50% 20%, rgba(0,0,0,0.1), rgba(0,0,0,0.8));
    background:-webkit-gradient(radial, 0 50%, 0 20%, from(rgba(0,0,0,0.1)), to(rgba(0,0,0,0.8)));
    z-index: 1;
}



