.view_full_size{
    position: absolute;
    bottom: 14px;
    left: 20px;
    /*width: 150px;*/
    text-align: left;
    /*background: #c00;*/
    color: #333;
    font-size: 18px;
    line-height: 1;
    z-index: 1;
    color: #bad5d8;
    overflow: hidden;
    -webkit-transition:-webkit-transform 0.2s;
    -moz-transition:-moz-transform 0.2s;
    transition:transform 0.2s;
}
.view_full_size i{
    float: left;
}
.view_full_size span{
    position: relative;
    float: left;
    top: 2px;
    left: -30px;
    padding-left: 5px;
    font-size: 12px;
    font-weight: 300;
    vertical-align: middle;
    opacity: 0;
    -webkit-transition:opacity 0.2s, left 0.2s;
    -moz-transition:opacity 0.2s, left 0.2s;
    transition:opacity 0.2s, left 0.2s;
}
.view_full_size:hover{
    color: #b1cbce;
    -webkit-transform: scale(1.2);
    -moz-transform: scale(1.2);
    transform: scale(1.2);
}
.view_full_size:hover span{
    left: 0;
    opacity: 1;
}

